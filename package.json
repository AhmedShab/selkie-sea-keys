{
  "name": "selkie-sea-keys",
  "version": "1.0.0",
  "description": "awesome",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "watch": "watchify . -o bundle.js",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mandy-kate/selkie-sea-keys"
  },
  "author": "mandy-kate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mandy-kate/selkie-sea-keys/issues"
  },
  "homepage": "https://github.com/mandy-kate/selkie-sea-keys",
  "dependencies": {
    "font-awesome": "^4.6.3",
    "react-fontawesome": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
